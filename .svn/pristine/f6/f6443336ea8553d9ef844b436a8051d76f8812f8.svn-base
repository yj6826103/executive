<script>
import viewDetails from "../CommonParts/viewDetails";
import seatLive from "../CommonParts/seatLive";
import seatQueue from "../CommonParts/seatQueue";
 import seatAlarm from '../CommonParts/seatAlarm'
import { mapGetters, mapMutations } from "vuex";
import qs from "qs";
export default {
  components: {
    viewDetails,
    seatLive,
    seatQueue,
    seatAlarm
  },
  data() {
    return {
      date: "",
      stndLevels:['1','2','3','4'],
      arpt: "PEK", // 默认首都机场
      showJobDialog: false,
      showFlgtDialog: false,
      isShowSeatLive: false, // 是否显示机位实况
      isShowSeatQueue: false, // 是否显示机位进港队列
      isShowSeatAlarm: false, // 是否显示机位占用预警
      flgtList: [],
      stndTreeList: [],
      stndFlgtList: [],
      regCode: null,
      rowIndex: 0,
      jobList: [],
      undoJoblist: [],

      stnd: null,
      stndObjectName: null,
      flgt: null,

      arrivalLogisticsJobTypes: [], // 进港任务类型
      departureLogisticsJobTypes: [] // 出港任务类型
    };
  },
  computed: {
    ...mapGetters([
      "orderArptCodes", //机场列表
      "orderArptCode" //机场
    ])
  },
  mounted() {
    this.$store.dispatch("get_SYS_ARPT_CODE"); //机场
    this.date = this.$formatDate(new Date(), "yyyy-MM-dd HH:mm:ss"); // 默认当前系统时间
    this.search();
    this.getAllJobType();
  },
  methods: {
    handleStndLevelsChange(value){
      console.log(this.stndLevels)
    },
    // 显示机位实况
    showSeatLive() {
      this.isShowSeatLive = true
    },
    // 显示机位进港队列
    showSeatQueue() {
        this.isShowSeatQueue = true
    },
    // 显示机位占用预警
    showSeatAlarm() {
        this.isShowSeatAlarm = true
    },
    getAllJobType() {
      let _this = this;
      _this.$http
        .get("/logisticsJob/arrivalLogisticsJobTypes", {
          headers: { Authorization: localStorage.getItem("token") }
        })
        .then(function(res) {
          if (res.data.status == "100") {
            _this.arrivalLogisticsJobTypes = res.data.data;
          }
        })
        .catch(function(err) {
          console.log(err);
        });
      _this.$http
        .get("/logisticsJob/departureLogisticsJobTypes", {
          headers: { Authorization: localStorage.getItem("token") }
        })
        .then(function(res) {
          if (res.data.status == "100") {
            _this.departureLogisticsJobTypes = res.data.data;
          }
        })
        .catch(function(err) {
          console.log(err);
        });
    },
    changeArptCode(val) {
      //获取机场
      this.$store.commit("set_orderArptCode", val);
    },
    // 通过二期航班PK查询保障任务
    showJob(flgt) {
      let _this = this;
      let param = qs.stringify({
        flgtPkey: flgt.flgtPkey
      });
      let url = "";
      if (flgt.flgtAOrD == "进港") {
        url = "/logisticsJob/arrivalLogisticsJobs";
      } else {
        url = "/logisticsJob/departureLogisticsJobs";
      }
      _this.flgt = flgt;

      _this.jobList = [];
      _this.$http
        .post(url, param, {
          headers: { Authorization: localStorage.getItem("token") }
        })
        .then(function(res) {
          if (res.data.status == "100") {
            _this.jobList = res.data.data;
            let undoJoblist = null;
            let allJobList = null;
            if (flgt.flgtAOrD == "进港") {
              allJobList = _this.arrivalLogisticsJobTypes;
            } else {
              allJobList = _this.departureLogisticsJobTypes;
            }
            for (let j = 0; j < allJobList.length; j++) {
              let dic = allJobList[j];
              let flag = true;
              for (let i = 0; i < _this.jobList.length; i++) {
                let job = _this.jobList[i];
                if (job.jobType == dic.dicCode) {
                  false;
                }
                break;
              }
              if (flag) {
                undoJoblist.push(dic);
              }
            }
            _this.undoJoblist = undoJoblist;
          }
          _this.showJobDialog = true;
        })
        .catch(function(err) {
          console.log(err);
        });
    },
    // 通过MATCH航班进港PK查询保障任务
    showAllJob(flgt) {
      let _this = this;
      let param = qs.stringify({
        fmAPkey: flgt.fmAPkey
      });

      _this.flgt = flgt;

      _this.jobList = [];
      _this.$http
        .post("/monitor/logisticsJobsByMatchPK", param, {
          headers: { Authorization: localStorage.getItem("token") }
        })
        .then(function(res) {
          if (res.data.status == "100") {
            _this.jobList = res.data.data.list;
            console.log(_this.jobList);
            _this.showJobDialog = true;
          }
        })
        .catch(function(err) {
          console.log(err);
        });
    },
    viewDetails(stnd, stndObjectName) {
      if (stnd.stndColor == "BLACK" || stnd.stndColor == "WHITE") {
        return;
      }

      let _this = this;
      let param = qs.stringify({
        date: _this.date,
        stndCode: stnd.stndCode,
        stndColor: stnd.stndColor
      });

      _this.stnd = stnd;
      _this.stndObjectName = stndObjectName;

      _this.regCode = null;

      _this.stndFlgtList = [];
      _this.$http
        .post("/monitor/getFlgtByStndAndTime", param, {
          headers: { Authorization: localStorage.getItem("token") }
        })
        .then(function(res) {
          if (res.data.status == "100") {
            _this.stndFlgtList = res.data.data.list;
          }
        })
        .catch(function(err) {
          console.log(err);
        });
      _this.showFlgtDialog = true;
    },
    search() {
      let _this = this;
      let param = qs.stringify({
        date: _this.date,
        arpt: _this.arpt,
        stndLevels: _this.stndLevels
      },{arrayFormat: 'repeat'});
      // 查询 非公务机机位当天航班
      _this.$http
        .post("/monitor/flightOfNonCompanyStand", param, {
          headers: {
            Authorization: localStorage.getItem("token"),
            page: 0,
            limit: 0
          }
        })
        .then(function(res) {
          if (res.data.status == "100") {
            _this.flgtList = res.data.data.list;
          } else {
            _this.flgtList = [];
          }
        })
        .catch(function(err) {
          _this.flgtList = [];
          console.log(err);
        });
      // 查询 所有机位
      _this.$http
        .post("/monitor/allStndStatus", param, {
          headers: { Authorization: localStorage.getItem("token") }
        })
        .then(function(res) {
          if (res.data.status == "100") {
            _this.stndTreeList = res.data.data.list;
          } else {
            _this.stndTreeList = [];
          }
        })
        .catch(function(err) {
          _this.stndTreeList = [];
          console.log(err);
        });
    },
    spanMethod({ row, column, rowIndex, columnIndex }) {
      if (columnIndex === 0) {
        let count = 0;
        let list = this.stndFlgtList;
        console.log(rowIndex + "," + columnIndex);
        if (this.regCode != row.flgtRegCode) {
          this.regCode = row.flgtRegCode;
          for (let i = 0; i < list.length; i++) {
            if (list[i].flgtRegCode === this.regCode) {
              count++;
            }
          }
          return [count, 1];
        } else {
          return [0, 0];
        }
      }
    }
  }
};
</script>

