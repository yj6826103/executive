<script>
import {mapGetters} from 'vuex'
import qs from 'qs'
export default {
    data(){
        return{
           totalNumber:0,//总条数
           pagesize:10,//每页的数据条数
           currentPages:1,//默认开始页面
           judNum:true,//判断分页
           list:[],
           itfFlgt:{
               flgtArpt:'PEK'
           },
           stm:[],
           stopArptList:[],
        }
    },
    created () {
        this.$store.dispatch("dataInit")
    },
    computed: {
        
    },
    mounted () {
        this.getdatas();
        this.getStopArptList()
    },
    methods: {
        clear(){
            this.itfFlgt={flgtArpt:'PEK'}
        },
        change(){
            console.log(this.itfFlgt.flgtAOrD)
        },
        getStopArptList(){
            let _this = this;
            let formData = new FormData();
            formData.append("dicType","SYS_ARPT_CODE"); // 停靠机场
            _this.$http.post(
               '/bizOrder/selectAirptPark',formData,{headers:{'Authorization':localStorage.getItem("token")}}
           ).then(function(res) {
               if(res.data.status == '100'){
                   _this.stopArptList = res.data.data;
               }
           });
        },
        currentPage(currentPage){//触发
            this.currentPages=currentPage
            this.getdatas()
        },
        getdatas(){
            let _this = this;
            _this.$http.post(
               '/acteam/itfFlgt',qs.stringify(_this.itfFlgt),{
                   headers:{'Authorization':localStorage.getItem("token"),'page':_this.currentPages,'limit':_this.pagesize}}
           ).then(function(res) {
                if(res.data.status == '100'){
                    _this.list = res.data.data.list
                    _this.totalNumber =res.data.data.total
                    if(parseInt(_this.totalNumber)>10){
                    _this.judNum = true
                    }
                    else{
                        this.judNum=false
                    }
                }else{
                    _this.list = [];
                    _this.totalNumber = 1;
                }
            })
            .catch(function(err) {
                const h = _this.$createElement;
                _this.$notify({
                    title: '系统提示',
                    message: h('i', { style: 'color: teal'}, res.data.msg)
                });
            });
        }
    }
};
</script>

