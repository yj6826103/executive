                                            v-bind:fileType="5" 
                                            v-bind:disabled="orderBocStatus==2" 
                                            v-bind:fileName="bizOrder.bocDoc.docName" 
                                            v-bind:serverFileName="bizOrder.bocDoc.docAddress" 
