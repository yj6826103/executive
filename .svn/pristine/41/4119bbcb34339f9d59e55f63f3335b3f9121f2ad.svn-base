                     this.totalNumber=parseInt(res.data.data.total)
                     this.dataList=res.data.data.list                    
                     if(parseInt(res.data.data.total)>10){
                       this.judNum=true
                    }else{
                        this.judNum=false
                    } 
                } else if(res.data.status == 402){
                     this.dataList=[]
                     this.judNum=false
                }
        })
