.ybq span::after {
  content: "";
  display: block;
  width: 60px;
  height: 60px;
  border-radius: 100%;
  border: 1px solid #eee;
  position: absolute;
  z-index: -1;
  left: 50%;
  right: 0;
  top: -20px;
  margin-left: -30px;
}

.rbq::before {
  content: "";
  font-size: 0;
  display: inline-block;
  width: 0;
  height: 0;
  border-top: 5px solid transparent;
  border-left: 8px solid #C0C4CC;
  border-bottom: 5px solid transparent;
  margin-right: 8px;
}

