                      <el-pagination 
                          v-if="judNum" 
                          @current-change="currentPage"
                          :page-size="pagesize" 
                          background 
                          layout="prev, pager, next,jumper" 
                          :total="totalNumber">
                      </el-pagination> 
                </div>
                <div class="btnbox">
                    <el-button type="warning" @click="selected">选  择</el-button>
                    <el-button @click="close" plain>取  消</el-button>
                </div>
