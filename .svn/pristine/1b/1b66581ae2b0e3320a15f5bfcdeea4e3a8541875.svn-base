                        <th><div class="tableWidth">停靠机场</div></th>
                        <th><div class="tableWidth">机型等级</div></th>
                        <th v-for="(item,index) in dataList[0].list" width="210"><div class="tableWidth90">{{" "+ item.dataDate+"   "+item.dataWeek}}</div></th>
