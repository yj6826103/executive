                            <div class="rbbq">
                                <div class="cen timeStyle">
                                    <label class="input-time">
                                        <el-date-picker v-model="jgData2" type="date" value-format="yyyy-MM-dd" :placeholder="$t('Pleaseselectdate')"  @change="timeChange2" :clearable="false"></el-date-picker>
                                        <i class="el-icon-date"></i>
