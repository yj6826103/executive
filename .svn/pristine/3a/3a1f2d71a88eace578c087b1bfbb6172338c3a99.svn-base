<script>
import {mapMutations,mapGetters} from 'vuex'
import singleFileUpload from '../other/SingleFileUpload'
export default {
    props: ['orderNo'],
    components: {
        singleFileUpload
    },
    data () {
        return {
           bizOrder:{},
           file:{},
           fileName:'上传附件信息'
        }
    },
    mounted () {    
       this.getData();
    },
    methods: {
        ...mapMutations([
            'clickSelectFalse'
        ]), 
        close(){//关闭弹框
            this.clickSelectFalse()
        },
        saveFileInfo(fileType,index,file){
            if(fileType == 5){
                this.fileName = file.oldName
                this.bizOrder.bocDoc = {
                    docName:file.oldName,
                    docAddress:file.newName,
                    docId:''
                }
            }
        },
        getData(){
            let _this = this;
            let formData = new FormData();
            formData.append("orderNo",_this.orderNo);
            _this.$http.post("/bizOrder/selectBreakNotify",formData,{headers:{'Authorization':localStorage.getItem("token")}}
            ).then(function(res){
                if(res.data.status == '100'){
                    _this.bizOrder = res.data.data.list[0];
                    _this.bizOrder.orderDate = _this.$formatDate(new Date(),"yyyy-MM-dd hh:mm:ss");
                    let flag = true
                    if(_this.bizOrder.basDocs!=null){
                        _this.bizOrder.basDocs.forEach(doc => {
                            if(doc.docType == '航班信息违约通知附件'){
                                _this.bizOrder.bocDoc = doc
                                flag = false
                            }
                        });
                    }
                    if(flag){
                        _this.bizOrder.bocDoc = {docName:'',docAddress:''}
                    }
                }else if(res.data.status == '402'){
                    _this.bizOrder = {};
                    const h = _this.$createElement;
                    _this.$notify({
                        title: '系统提示',
                        message: h('i', { style: 'color: teal'}, '未查询到该违约订单！')
                    });
                }
            }).catch(function(err) {
                console.log(err);
            });
        },
        submit(){
            let _this = this;
            let formData = new FormData();
            let order = JSON.parse(JSON.stringify(_this.bizOrder));
            formData.append("bizOrderStr",JSON.stringify(order));
            if(_this.file!=null){
                formData.append('docId',_this.file);
            }
             _this.$http.post("/bizOrder/orderToNotify",formData,{headers:{'Authorization':localStorage.getItem("token")}}
            ).then(function(res){
                const h = _this.$createElement;
                _this.$notify({
                    title: '系统提示',
                    message: h('i', { style: 'color: teal'}, res.data.msg)
                });
                _this.clickSelectFalse();
                _this.$emit('success');
            }).catch(function(err) {
                console.log(err);
                _this.clickSelectFalse();
            });
        }
        
    }
}
</script>

